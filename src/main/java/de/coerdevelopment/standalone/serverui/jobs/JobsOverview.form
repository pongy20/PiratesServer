<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="de.coerdevelopment.standalone.serverui.jobs.JobsOverview">
  <grid id="27dc6" binding="contentPanel" layout-manager="BorderLayout" hgap="0" vgap="0">
    <constraints>
      <xy x="20" y="20" width="500" height="400"/>
    </constraints>
    <properties/>
    <border type="none"/>
    <children>
      <grid id="1c792" binding="headerPanel" layout-manager="FlowLayout" hgap="5" vgap="5" flow-align="1">
        <constraints border-constraint="North"/>
        <properties/>
        <border type="none"/>
        <children>
          <component id="1ae7" class="javax.swing.JComboBox" binding="jobsComboBox" custom-create="true">
            <constraints/>
            <properties/>
          </component>
          <component id="ab514" class="javax.swing.JButton" binding="detailsButton">
            <constraints/>
            <properties>
              <text value="details"/>
            </properties>
          </component>
        </children>
      </grid>
      <grid id="a3c1b" binding="centerPanel" layout-manager="BorderLayout" hgap="0" vgap="0">
        <constraints border-constraint="Center"/>
        <properties/>
        <border type="none"/>
        <children>
          <scrollpane id="4132c" binding="tableScrollPane">
            <constraints border-constraint="Center"/>
            <properties/>
            <border type="none"/>
            <children>
              <component id="f93e7" class="javax.swing.JTable" binding="jobsTable" custom-create="true">
                <constraints/>
                <properties/>
              </component>
            </children>
          </scrollpane>
        </children>
      </grid>
      <grid id="4e58d" binding="footerPanel" layout-manager="FlowLayout" hgap="5" vgap="5" flow-align="1">
        <constraints border-constraint="South"/>
        <properties/>
        <border type="none"/>
        <children>
          <component id="40c07" class="javax.swing.JLabel" binding="maxEntriesLabel">
            <constraints/>
            <properties>
              <text value="Max. Entries:"/>
            </properties>
          </component>
          <component id="3a132" class="javax.swing.JComboBox" binding="maxEntryCombo">
            <constraints/>
            <properties>
              <model>
                <item value="1"/>
                <item value="10"/>
                <item value="25"/>
                <item value="50"/>
                <item value="100"/>
                <item value="500"/>
              </model>
              <selectedIndex value="1"/>
            </properties>
          </component>
          <component id="1d0f0" class="javax.swing.JButton" binding="refreshButton" default-binding="true">
            <constraints/>
            <properties>
              <text value="refresh"/>
            </properties>
          </component>
        </children>
      </grid>
    </children>
  </grid>
</form>
