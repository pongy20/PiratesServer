<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="de.coerdevelopment.standalone.serverui.playermanagement.PlayerPanel">
  <grid id="27dc6" binding="contentPanel" layout-manager="BorderLayout" hgap="0" vgap="0">
    <constraints>
      <xy x="20" y="20" width="500" height="400"/>
    </constraints>
    <properties/>
    <border type="empty">
      <size top="10" left="10" bottom="10" right="10"/>
    </border>
    <children>
      <grid id="dec59" binding="headerPanel" layout-manager="BorderLayout" hgap="0" vgap="0">
        <constraints border-constraint="North"/>
        <properties/>
        <border type="none"/>
        <children>
          <component id="75d66" class="javax.swing.JLabel" binding="headerLabel">
            <constraints border-constraint="North"/>
            <properties>
              <font size="20" style="1"/>
              <text value="Players/Clients:"/>
            </properties>
          </component>
          <component id="2757d" class="javax.swing.JLabel" binding="clientsConnectedLabel">
            <constraints border-constraint="West"/>
            <properties>
              <font size="14"/>
              <text value="Clients connected: xxxx"/>
            </properties>
          </component>
          <component id="c8933" class="javax.swing.JButton" binding="refreshButton">
            <constraints border-constraint="East"/>
            <properties>
              <text value="refresh"/>
            </properties>
          </component>
        </children>
      </grid>
      <scrollpane id="d5d4f" binding="centerScrollPanel">
        <constraints border-constraint="Center"/>
        <properties/>
        <border type="none"/>
        <children>
          <component id="d5180" class="javax.swing.JList" binding="playersList">
            <constraints/>
            <properties/>
          </component>
        </children>
      </scrollpane>
    </children>
  </grid>
</form>
